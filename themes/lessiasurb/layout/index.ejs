<head>
<%- partial('partial/metadata', {
    config: config,
    theme: theme,
    canonical: page.path
  }) -%>
<%- partial('partial/pre_body_scripts') -%>
</head>
<body class="homepage">
  <header>
    <div class="title">
      <h1><span class="darker">ALESSIA</span> SORBO</h1>
      <h2>Writing <span class="lighter">About</span> Things</h2>
    </div>
    <a class="down" href="#posts" rel="nofollow" title="go to posts"></a>
    <%- partial('partial/theme_switcher', {}, {cached: true}) -%>
  </header>
  <a name="posts"></a>
  <div class="list-item">
    <div class="presentation">
      Writing about Movies, Travel, People and other Things.<br>
      Seeking the deepest concepts while being informative.
    </div>
  </div>
  <%- partial('partial/category_list') -%>
  <%- partial('partial/post_list') -%>
  <%- partial('partial/list_footer', {}, {cached: true}) -%>
  <%- partial('partial/post_body_scripts') -%>
</body>
